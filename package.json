{
  "name": "easy-note",
  "version": "0.1.0",
  "description": "",
  "main": "main.js",
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=dev webpack-dev-server --mode development",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "serve": "run-electron .",
    "start-e": "cross-env START_HOT=1 yarn start",
    "win": "cross-env NODE_ENV=production build --win --x64",
    "linux": "build --linux",
    "mac": "build --mac",
    "test": "jest",
    "test:watch": "npm run test --watch",
    "test:cover": "npm run test --coverage"
  },
  "build": {
    "productName": "Easy-Note",
    "appId": "omatic.app.easy.note",
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "files": [
      "build/**/*",
      "node_modules/",
      "main.js",
      "package.json"
    ],
    "win": {
      "asar": false,
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "maintainer": "Andrey Savchuk <andrej.savchuk@gmail.com>",
      "target": [
        "deb",
        "AppImage"
      ]
    },
    "directories": {
      "buildResources": "resources",
      "output": "build"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "sideEffects": false,
